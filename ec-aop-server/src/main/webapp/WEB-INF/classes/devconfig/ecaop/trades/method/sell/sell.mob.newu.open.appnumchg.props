#定义processors
#流程控制
ecaop.core.method.opnc.processors=@NumRoute\
                           @FlowProcessor(ecaop.core.method.opnc.flowConfig)
#numSwitch 0 号卡中心流程 1原有流程
ecaop.core.method.opnc.flowConfig=[\
{id:"01", processor:"SimpleProcessor", selector:"GlobalParam(numSwitch)", transitions:"0->03 1->02"},\
{id:"02", processor:"AopHandler(ecaop.trades.opnc4old.processors)", selector:"Noop", transitions:"*->STOP"},\
{id:"03", processor:"AopHandler(ecaop.trades.opnc4center.processors)", selector:"Noop", transitions:"*->STOP"}\
]
#定义processors 老流程
ecaop.trades.opnc4old.processors=@OpenApplyProcessor()
#定义processors 号卡中心流程
ecaop.trades.opnc4center.processors=@GDOpenAccountNumberCenter()

