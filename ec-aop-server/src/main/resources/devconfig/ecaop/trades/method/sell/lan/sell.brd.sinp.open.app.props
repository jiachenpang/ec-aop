{id:"04", processor:"AopHandler(ecaop.mall.sub.B6bsop.processors)", selector:"Noop", transitions:"*->STOP"},\
{id:"05", processor:"AopHandler(ecaop.mall.sub.bsop.processors)", selector:"Noop", transitions:"*->STOP"}\
]
