                                  @FlowProcessor(ecaop.core.method.baoa.flowConfig)

#配置跳转
ecaop.core.method.baoa.flowConfig=[\
{id:"01", processor:"SimpleProcessor", selector:"GlobalParam(opeSysType)", transitions:"2->02 *->03"},\
{id:"02", processor:"AopHandler(ecaop.mall.sub.CBbaoa.processors)", selector:"Noop", transitions:"*->STOP"},\
{id:"03", processor:"SimpleProcessor", selector:"GlobalParam(routeCode)", transitions:"N6->04 N25->05"},\
{id:"04", processor:"AopHandler(ecaop.mall.sub.N6baoa.processors)", selector:"Noop", transitions:"*->STOP"},\
{id:"05", processor:"AopHandler(ecaop.mall.sub.CBbaoa.processors)", selector:"Noop", transitions:"*->STOP"}\
]

ecaop.mall.sub.CBbaoa.processors=@CBbrdAddOpenPreCommit()
ecaop.mall.sub.N6baoa.processors=@N6brdAddOpenPreCommit()


